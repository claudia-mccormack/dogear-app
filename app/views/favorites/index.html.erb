<h1>Your Favorites:</h1>
<ul>
  <% @favorites.each do |favorite| %>
    <li><%= favorite.book.title %></li>
    <li><a href="/books/<%= favorite.book.id %>"><img src="<%= favorite.book.cover %>" alt=""></a></li>
    <li><%= link_to "Remove", "/favorites", method: "destroy" %></li>
  <% end %>
</ul>
